print("=== MATH LIBRARY TEST ===")

# 1. Konstanta PI
set pi = math.pi()
print("PI Value : " + pi)

# 2. Trigonometri (Sin 90 derajat / PI/2 radian)
set angle = pi / 2
print("Sin(PI/2): " + math.sin(angle)) # Harusnya 1.0

# 3. Pangkat & Akar
set base = 5
set power = 3
print("5^3      : " + math.pow(base, power)) # Harusnya 125
print("Sqrt(144): " + math.sqrt(144))        # Harusnya 12

# 4. Absolut (Nilai mutlak)
set negative = -99.5
print("Abs(-99.5): " + math.abs(negative))

# 5. Kalkulasi Kompleks
# Rumus Pythagoras: c = sqrt(a^2 + b^2)
set a = 3
set b = 4
set c = math.sqrt(math.pow(a, 2) + math.pow(b, 2))
print("Pythagoras (3,4,?): " + c)
